{
  "name": "@odata2ts/converter-runtime",
  "version": "0.4.1",
  "description": "Provides necessary implementations for using odata2ts converters at runtime",
  "keywords": [
    "odata2ts",
    "odata",
    "converter",
    "runtime",
    "typescript",
    "ts",
    "javascript",
    "js"
  ],
  "repository": "git@github.com:odata2ts/converter.git",
  "license": "MIT",
  "author": "texttechne",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "*.md",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "check-circular-deps": "madge ./src --extensions ts --circular",
    "clean": "rimraf lib coverage",
    "compile": "tsc",
    "prepublish": "yarn build",
    "test": "vitest run"
  },
  "dependencies": {
    "@odata2ts/converter-api": "^0.2.0"
  },
  "devDependencies": {
    "@odata2ts/converter-luxon": "^0.2.1",
    "@odata2ts/converter-v2-to-v4": "^0.5.1",
    "@types/node": "^17.0.23",
    "luxon": "^3.5.0",
    "rimraf": "^3.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.6.3",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
