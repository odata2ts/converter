{
  "name": "@odata2ts/converter-big-number",
  "version": "0.2.1",
  "description": "bignumber.js based odata2ts compatible converter for OData's `Edm.Int64` and `Edm.Decimal` types",
  "keywords": [
    "bignumber.js",
    "converter",
    "odata2ts",
    "odata",
    "int64",
    "decimal",
    "ts"
  ],
  "repository": "git@github.com:odata2ts/converter.git",
  "license": "MIT",
  "author": "texttechne",
  "type": "commonjs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "*.md",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf lib coverage",
    "compile": "tsc",
    "prepublish": "yarn build",
    "test": "vitest run"
  },
  "dependencies": {
    "@odata2ts/converter-api": "^0.2.0",
    "bignumber.js": ">1"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "bignumber.js": "^9.1.1",
    "rimraf": "^3.0.2",
    "typescript": "4.6.3",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
