{
  "name": "@odata2ts/converter-decimal",
  "version": "0.2.5",
  "description": "decimal.js based odata2ts compatible converter for OData's `Edm.Int64` and `Edm.Decimal` types",
  "keywords": [
    "decimal.js",
    "converter",
    "odata2ts",
    "odata",
    "int64",
    "decimal",
    "ts"
  ],
  "repository": "git@github.com:odata2ts/converter.git",
  "license": "MIT",
  "author": "texttechne",
  "type": "commonjs",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "*.md",
    "lib",
    "LICENSE"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "check-circular-deps": "madge ./src --extensions ts --circular",
    "clean": "rimraf lib coverage",
    "compile": "tsc",
    "prepublish": "yarn build",
    "test": "vitest run"
  },
  "dependencies": {
    "@odata2ts/converter-api": "^0.2.4"
  },
  "devDependencies": {
    "decimal.js": "^10.4.3"
  },
  "peerDependencies": {
    "decimal.js": ">1"
  },
  "publishConfig": {
    "access": "public"
  }
}
